<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="SuiteFromXML" verbose="1">


    <test name="Search in FF">
        <parameter name="browser" value="FF"/>
        <classes>
            <class name="tests.GoogleSearchTest">
                <methods>
                    <include name="test_SearchWithParameter"/>
                </methods>
            </class>
        </classes>

    </test>


    <test name="Search in Chrome">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="tests.GoogleSearchTest">
                <methods>
                    <include name="test_SearchWithParameter"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
